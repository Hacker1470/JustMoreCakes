{
  "schemaVersion": 1,
  "id": "jmc",
  "version": "${version}",
  "name": "Just More Cakes!",
  "description": "A simple mod that adds 30+ new cakes to Minecraft",
  "authors": [
    "MincraftEinstein"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/just-more-cakes",
    "sources": "https://github.com/MincraftEinstein/JustMoreCakes",
    "issues": "https://github.com/MincraftEinstein/JustMoreCakes/issues"
  },
  "license": "All rights reserved",
  "icon": "pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "einstein.jmc.JustMoreCakesFabric"
    ],
    "client": [
      "einstein.jmc.JustMoreCakesClient"
    ],
    "fabric-datagen": [
      "einstein.jmc.JustMoreCakesData"
    ],
    "jei_mod_plugin": [
      "einstein.jmc.compat.jei.ModJEIPlugin"
    ],
    "rei_client": [
      "einstein.jmc.compat.rei.REIClientPlugin"
    ],
    "rei_common": [
      "einstein.jmc.compat.rei.REIPlugin"
    ]
  },
  "mixins": [
    "jmc.fabric.mixins.json",
    "jmc.mixins.json"
  ],
  "accessWidener": "jmc.accesswidener",
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": ">=0.83.0",
    "minecraft": "1.20.x",
    "java": ">=17",
    "forgeconfigapiport": ">=7.0.0"
  },
  "suggests": {
    "jei": ">=14.0.0.11",
    "configured": ">=2.0.2",
    "cakechomps": ">=6.0.1+1.19.4",
    "roughlyenoughitems": ">=12.0.625"
  },
  "custom": {
    "catalogue": {
      "banner": "pack.png"
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/gSsaFAvrBM"
      }
    }
  }
}
